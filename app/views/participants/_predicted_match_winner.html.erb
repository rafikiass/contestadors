<h4>Winner of <%= title %></h4>
<% winner_team = @predictions[:stages][match][:winner_team] -%>
<% stage = Predictable::Championship::Stage.where(:description => stage_name).last -%>
<% if winner_team -%>
    <% if winner_team.objectives_meet_for.empty? -%>
      <div>
    <% else -%>
      <div class="<%= stage_objectives_meet_div_class_for(winner_team.objectives_meet_for, stage) %>">
    <% end -%>
    <%= winner_team.name %><br/><br/>
  </div>
<% else -%>
  <p>Not predicted.</p>
<% end -%>
