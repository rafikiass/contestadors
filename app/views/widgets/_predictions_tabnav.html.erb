<% render_tabnav :predictions, :generate_css => false do
  
    if @wizard
      @wizard.all_available_steps.each do |step|
        add_tab do |t|
          t.named step.label
          t.titled step.label
          t.links_to prediction_menu_link(@contest.permalink, step.type, step.id)
          t.highlights_on step.highlight_conditions
        end
      end
    end
  end
%>
