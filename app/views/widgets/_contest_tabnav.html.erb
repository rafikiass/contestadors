<%       
render_tabnav :contest, 
              :generate_css => false do
  contest_tabnav_items.each do |menu_item|
    add_tab do |t|
      t.named menu_item[:name]
      t.titled menu_item[:tip]
      t.links_to menu_item[:path]
      menu_item[:highlight_conditions].each {|condition| t.highlights_on condition}
    end
  end 
end 
%>