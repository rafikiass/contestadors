<%  
if before_contest_participation_ends
  render_tabnav :invitations,
                :generate_css => false do

    received_invitations_tabnav_items.each do |menu_item|
      add_tab do |t|
        t.named menu_item[:label]
        t.titled menu_item[:tip]
        t.links_to menu_item[:path]
        menu_item[:highlight_conditions].each {|condition| t.highlights_on condition}
      end
    end
  end
end
%>